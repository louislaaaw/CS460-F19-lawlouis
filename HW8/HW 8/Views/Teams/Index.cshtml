@model IEnumerable<HW_8.Models.Team>

@{
    ViewBag.Title = "Index";
}

<h2>Teams</h2>

<script>
    function ShowAthletes(data) {
        $("#athletes").append();
    }
</script>

@foreach (var item in Model) {
    <button class="btn btn-default btn-lg" type="button" onclick="ShowAthletes()">@Html.DisplayFor(modelItem => item.TeamName)</button>
}

<div id="athletes">

</div>